{
  "name": "modelsnexus",
  "private": true,
  "version": "1.0.29",
  "license": "UNLICENSED",
  "description": "A cross-platform model browser and prompt-viewer for Stable Diffusion models",
  "author": "Badaxiom",
  "main": "main/main.js",
  "type": "module",
  "scripts": {
    "build:renderer": "cd renderer && yarn install && yarn build",
    "build:main": "tsc -p .",
    "build": "yarn build:renderer && yarn build:main",
    "prepackage": "rimraf out",
    "package": "yarn prepackage && yarn build && electron-forge package",
    "dist": "yarn package && electron-builder --prepackaged out/modelsnexus-win32-x64",
    "make": "yarn package && electron-forge make",
    "start": "yarn package && electron-forge start",
    "lint": "eslint . --ext .ts,.tsx",
    "update-hashmap": "ts-node main/utils/buildCivitaiHashMap.ts",
    "clean:win": "for /r %i in (*.js *.js.map) do @(if /i not \"%~nxi\"==\"postcss.config.js\" if /i not \"%~nxi\"==\"tailwind.config.js\" if /i not \"%~nxi\"==\"vite.config.js\" if /i not \"%~fi\"==\"%cd%\\node_modules\\%~nxi\" if /i not \"%~fi\"==\"%cd%\\dist\\%~nxi\" del \"%~fi\")"
  },
  "dependencies": {
    "@c4312/blake3-internal": "^3.0.0",
    "@c4312/blake3-native": "^3.0.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.2.0",
    "axios": "^1.10.0",
    "blake3": "^3.0.0",
    "cheerio": "^1.1.0",
    "electron-squirrel-startup": "^1.0.1",
    "exifreader": "^4.31.1",
    "fs-extra": "^11.3.0",
    "lucide-react": "^0.525.0",
    "pngjs": "^7.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-rpm": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron/fuses": "^1.8.0",
    "@eslint/js": "^9.30.1",
    "@types/fs-extra": "^11.0.4",
    "@vitejs/plugin-react": "^4.6.0",
    "electron": "37.2.3",
    "electron-builder": "^26.0.12",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3",
    "vite": "6"
  },
  "resolutions": {
    "@c4312/blake3-internal": "3.0.0",
    "@c4312/blake3-native": "3.0.0",
    "blake3-wasm": "2.1.5"
  },
  "forge": {
    "packagerConfig": {
      "asar": {
        "unpack": [
          "resources/*.exe"
        ]
      },
      "extraResource": [
        "resources/sd-prompt-reader-cli.exe",
        "resources/sd-prompt-reader.exe"
      ]
    }
  },
  "build": {
    "directories": {
      "buildResources": "resources"
    },
    "appId": "com.badaxiom.modelsnexus",
    "productName": "Models Nexus",
    "files": [
      "**/*"
    ],
    "extraResources": [
      {
        "from": "resources",
        "to": "resources",
        "filter": [
          "*.exe"
        ]
      }
    ],
    "asarUnpack": [
      "resources/*.exe"
    ],
    "compression": "store",
    "win": {
      "target": [
        {
          "target": "zip",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "resources/icon.ico"
    }
  }
}
